{
  "name": "data-pipeline-nodejs",
  "version": "0.1.0",
  "description": "Bindinngs for data-pipeline crate",
  "main": "index.js",
  "scripts": {
    "build": "cargo-cp-artifact -nc index.node -- cargo build --message-format=json-render-diagnostics",
    "build-debug": "npm run build --",
    "build-release": "cargo-cp-artifact -nc build/Release/index.node -- cargo build --message-format=json-render-diagnostics --release",
    "build-release-opt": "cargo-cp-artifact -nc build/Release/index.node -- cargo +nightly build -Z build-std=std,panic_abort -Z build-std-features=panic_immediate_abort --message-format=json-render-diagnostics --release",
    "install": "exit 0",
    "test": "cargo test"
  },
  "author": "julio.gonzalez@datadoghq.com",
  "license": "ISC",
  "devDependencies": {
    "cargo-cp-artifact": "^0.1"
  },
  "files": [
      "./build/Release/index.node",
      "./index.js"
  ]
}
